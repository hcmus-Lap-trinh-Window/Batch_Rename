<UserControl x:Class="RuleWindow.AddPrefixRuleWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RuleWindow"
             mc:Ignorable="d" 
             Background="White"
             d:DesignHeight="450" d:DesignWidth="800" Loaded="LoadingFirstly">
    <StackPanel Margin="10">
        <TextBlock FontWeight="Bold" HorizontalAlignment="left">Add Prefix:</TextBlock>
        <TextBox x:Name="prefixInput" Text="{Binding Prefix}" TextChanged="prefixInput_TextChanged"
                  HorizontalAlignment="Stretch"/>
    </StackPanel>
</UserControl>
